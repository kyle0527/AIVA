{
  "cache_strategy": "hybrid",
  "cache_ttl_seconds": 7200,
  "max_cache_size": 5000,
  "max_concurrent_operations": 12,
  "operation_timeout_seconds": 20.0,
  "batch_size": 200,
  "connection_pool_size": 30,
  "connection_pool_timeout": 3.0,
  "enable_performance_monitoring": true,
